    "package-mac": "electron-packager ./ --platform=darwin --arch=x64 --overwrite --icon ./img/icon.icns --out ./dist-package --ignore=dist-package  --ignore=page/node_modules --ignore=page/build-tools --ignore=page/dev-helpers --ignore=page/src --ignore=page/swagger-editor-dist-package --ignore=page/test",
